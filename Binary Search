#include<stdio.h>
int main()
{
    int n=20,a[n],i,s,low,high,mid,count=0;
    printf("Enter the size of n = ");
    scanf("%d",&n);
    printf("Enter %d sorted elements only = ",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the number you want to search = ");
    scanf("%d",&s);
    
    low=0;
    high=n-1;
    
    while(low <= high)
    {
        int mid = (low + high) / 2; 
        if (s > a[mid]) 
        {
            low = mid + 1; 
        }
        else if (s < a[mid]) 
        {
            high = mid - 1; 
        }
        else 
        {
            printf("%d is present at %d position\n", s,mid);
            count++;
            break; 
        }
    }

    
    if (count == 0) {
        printf("%d is not present\n", s);
    }

    return 0;
}
